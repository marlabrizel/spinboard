<%= form_for(@user) do |f| %>
  <%= f.label :email_address %>
  <%= f.text_field :email_address %> <br>

  <%= f.label :password %>
  <%= f.password_field :password %> <br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %> <br>

  <%= f.submit "Sign Up" %>
<% end %>
